	<queryString>
		<![CDATA[SELECT tb_detail_penjualan.id_produk, tb_detail_penjualan.nama_barang, tb_detail_penjualan.id_penjualan, tb_detail_penjualan.Toko, tb_detail_penjualan.sub_total, tb_detail_penjualan.Stock_Awal, tb_detail_penjualan.Stock_Akhir, DATE_FORMAT( tb_detail_penjualan.Tanggal,  '%M 20%y' )  AS bln, tb_penjualan.Diskon, DATE_FORMAT( NOW( ) ,  '%d-%M-20%y' ) AS tgl, tb_detail_penjualan.Tanggal
FROM tb_detail_penjualan
INNER JOIN tb_penjualan ON tb_detail_penjualan.id_penjualan = tb_penjualan.Id_Penjualan
