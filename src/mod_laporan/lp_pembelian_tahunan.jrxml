	<queryString>
		<![CDATA[SELECT tb_detail_pembelian.id_produk, tb_detail_pembelian.nama_produk, tb_detail_pembelian.id_pembelian, tb_detail_pembelian.Toko, tb_detail_pembelian.sub_total, tb_detail_pembelian.Stock_Awal, tb_detail_pembelian.Jumlah_Stock, DATE_FORMAT( tb_detail_pembelian.Tanggal,  '20%y' )  AS thn, DATE_FORMAT( NOW( ) ,  '%d-%M-20%y' ) AS tgl
FROM tb_detail_pembelian
INNER JOIN tb_pembelian ON tb_detail_pembelian.id_pembelian = tb_pembelian.Id_Pembelian
WHERE DATE_FORMAT( tb_pembelian.Tanggal,  '20%y' ) = $P{pThn}
AND tb_detail_pembelian.Toko =  $P{pToko}]]>
